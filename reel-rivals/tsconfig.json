{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    "**/*.jsx",  
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "app/signup.tsx",
    "app/login.tsx"  
  ]
}